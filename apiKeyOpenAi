npm install openai
import OpenAI from "openai";

const openai = new OpenAI({
  apiKey: "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",
});

const completion = openai.chat.completions.create({
  model: "gpt-4o-mini",
  store: true,
  messages: [
    {"role": "user", "content": "write a haiku about ai"},
  ],
});

completion.then((result) => console.log(result.choices[0].message));


pip install openai
from openai import OpenAI

client = OpenAI(
  api_key="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"
)

completion = client.chat.completions.create(
  model="gpt-4o-mini",
  store=True,
  messages=[
    {"role": "user", "content": "write a haiku about ai"}
  ]
)

print(completion.choices[0].message);
